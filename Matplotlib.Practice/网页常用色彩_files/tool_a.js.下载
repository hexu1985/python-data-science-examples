    init: function () {
        var loc = $("#loc");
        var tNum = $("#tnum").attr("n");
        if (!loc) return;
        switch (tNum) {
            case "1":
                loc.html(this.type1);
                break;
            case "3":
                loc.html(this.type3);
                break;
            case "4":
                loc.html(this.type4);
                break;
            default:
                loc.html(this.type1);
                break;
        }
    }
}
loc_a.init();

